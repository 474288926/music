r\" autoplay=\"autoplay\" @timeupdate=\"updataTime\" :src=\"currentSong.Url.url\" ></audio>\r\n  </el-affix>\n\n</template>\r\n\n<script>\r\n  import { mapGetters, mapMutations } from 'vuex'\r\n  \r\n  export default{\r\n    name: 'Buttom',\r\n    data(){\r\n      return {\r\n          songReady: false,\r\n          currentTime: 0\r\n      }\r\n    },\r\n    computed:{\r\n      ...mapGetters([\r\n        'fullScreen',\r\n        'playList',\r\n        'currentSong',\r\n        'playing',\r\n        'currentIndex'\r\n      ]),\r\n      playIcon() {\r\n        return this.playing ? 'el-icon-video-pause' : 'el-icon-video-play'\r\n      },\r\n      disableCls() {\r\n        return this.songReady ? '' : 'disable'\r\n      }\r\n    },\r\n    methods:{\r\n      ...mapMutations({\r\n        setPlayState: 'SET_PLAYING_STATE',\r\n        setCurrentIndex: 'SET_CURRENT_INDEX'\r\n      }),\r\n      togglePlaying(){\r\n        if(!this.songReady) {\r\n          return\r\n        }\r\n        this.setPlayState(!this.playing)\r\n        // console.log(this.$store.state.playing)\r\n      },\r\n      prev(){\r\n        if(!this.songReady) {\r\n          return\r\n        }\r\n        let index = this.currentIndex - 1\r\n        if(index === -1) {\r\n          index = this.playList.length - 1\r\n        }\r\n        this.setCurrentIndex(index)\r\n        if(!this.playing){\r\n          this.togglePlaying()\r\n        }\r\n      },\r\n      next(){\r\n        if(!this.songReady) {\r\n          return\r\n        }\r\n        let index = this.currentIndex + 1\r\n         if(index === this.playList.length) {\r\n           index = 0\r\n         }\r\n         this.setCurrentIndex(index)\r\n         if(!this.playing){\r\n           this.togglePlaying()\r\n         }\r\n      },\r\n      ready() {\r\n        this.songReady = true\r\n      },\r\n      error() {\r\n        this.songReady = true\r\n      },\r\n      updataTime(e) {\r\n        this.currentTime = e.target.currentTime\r\n      },\r\n    },\r\n    watch:{\r\n      currentSong() {\r\n        this.$nextTick(() => {\r\n          this.$refs.audio.play()\r\n        })\r\n      },\r\n      playing(newVal) {\r\n        const audio = this.$refs.audio\r\n        this.$nextTick(() => {\r\n          newVal ? audio.play() : audio.pause()\r\n        })\r\n      }\r\n    }\r\n  }\n</script>\n\n<style scoped>\r\n  #player{\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n  .player{\r\n    display: flex;\r\n    width: 50%;\r\n  }\r\n  .mini-play{\r\n    background-color: #175B9A;\r\n  }\r\n  .normal-player{\r\n    display: flex;\r\n  }\r\n  .top{\r\n    display: flex;\r\n  }\r\n  .top i{\r\n    font-size: 26px;\r\n  }\r\n  .center{\r\n    display: flex;\r\n    background-color: red;\r\n  }\r\n  .center img{\r\n    width: 8%;\r\n  }\r\n  .disable{\r\n    background-color: red;\r\n  }\r\n  .center-box{\r\n    width: 80%;\r\n    background-color: #475669;\r\n  }\r\n  .bottom{\r\n    width: 100%;\r\n    background-color: #175B9A;\r\n  }\n</style>\n"]}]}