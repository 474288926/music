= res.list\r\n        })\r\n      },\r\n      getList(value = 19723756){\r\n                return new Promise((resolve) => {\r\n                  let ids = []\r\n                  let songs = []\r\n                  getListDetail(value).then(res => {\r\n                    this.list = res.playlist\r\n                    // console.log(res)\r\n                    res.privileges.forEach(id => {\r\n                      ids.push(id.id)\r\n                    })\r\n                    getSongDetail(ids).then(res => {\r\n                      res.songs.forEach(value => {\r\n                        songs.push(value)\r\n                        this.flag++\r\n                      })\r\n                    })\r\n                    // this.$set(res, 'songs', songs)\r\n                    this.list.songs = songs\r\n                    console.log(this.list, '111111')\r\n                  })\r\n                  resolve()\r\n                })\r\n        },\r\n        \r\n        activelist(id){\r\n          this.flag = 0\r\n          this.getList(id)\r\n        }\r\n        \r\n    }\r\n  }\n</script>\n\n<style scoped>\r\n  #top-list{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    overflow-y: scroll;\r\n    height: 88vh;\r\n  }\r\n  .top-list{\r\n    display: flex;\r\n    justify-content: center;\r\n    width: 50%;\r\n    border-bottom: #d5d5d5 1px solid;\r\n  }\r\n  .fl{\r\n    padding-top: 40px;\r\n    border-left: #d5d5d5 1px solid;\r\n    border-right: #d5d5d5 1px solid;\r\n    width: 25%;\r\n  }\r\n  .fr{\r\n    flex: 1;\r\n    border-right: #d5d5d5 1px solid;\r\n    padding: 3% 3%;\r\n  }\n</style>\n"],"sourceRoot":""}]}