 <div class='top-list'>\r\n      <div class=\"fl\">\r\n        <top-fl :list=\"toplist\" title='云音乐特色榜' @abc = 'activelist' />\r\n        <top-fl :list=\"toplist\" title='全球媒体榜' start='4' :end='toplist.length' @abc = 'activelist'/>\r\n      </div>\r\n      <div class=\"fr\">\r\n        <top-list-content :list=\"list\" />\r\n      </div>\r\n    </div>\r\n  </div>\n</template>\n\n<script>\r\n  import TopFl from './childComps/TopFl.vue'\r\n  import TopListContent from './childComps/TopListContent.vue'\r\n  \r\n  import { getTopList, getListDetail, getSongDetail, getSongUrl } from '../../network/toplist.js'\r\n  export default{\r\n    name: 'TopList',\r\n    components:{\r\n      TopFl,\r\n      TopListContent\r\n    },\r\n    data(){\r\n      return {\r\n        list: [],\r\n        toplist: [],\r\n        id: '',\r\n        url: [],\r\n        songs: []\r\n      }\r\n    },\r\n    created() {\r\n      this.getTopList()\r\n      this.getList()\r\n    },\r\n    methods:{\r\n      getTopList(){\r\n        getTopList().then(res => {\r\n          this.toplist = res.list\r\n        })\r\n      },\r\n      getList(value = 19723756){\r\n                return new Promise((resolve) => {\r\n                  let ids = []\r\n                  getListDetail(value).then(res => {\r\n                    // console.log(res)\r\n                    this.list = res.playlist\r\n                    res.privileges.forEach(id => {\r\n                      ids.push(id.id)\r\n                    })\r\n                    getSongDetail(ids).then(res => {\r\n                      res.songs.forEach(value => {\r\n                        this.songs.push(value)\r\n                      })\r\n                    })\r\n                    getSongUrl(ids).then(res => {\r\n                        res.data.forEach(value => {\r\n                          this.url.push(value)\r\n                        })\r\n                    })\r\n                    for(let i = 0; i < this.songs.length; i++){\r\n                      if(i < 100){\r\n                        console.log(123)\r\n                      }\r\n                    }\r\n                    this.list.songs = this.songs\r\n                    this.list.songurl = this.url\r\n                    console.log(this.list, '111111')\r\n                  })\r\n                  resolve()\r\n                })\r\n                \r\n              \r\n\r\n        },\r\n        \r\n        activelist(){\r\n          // console.log(this.$route.query.id)\r\n          // console.log(index)\r\n          this.getList(this.$route.query.id)\r\n        }\r\n        \r\n    }\r\n  }\n</script>\n\n<style scoped>\r\n  #top-list{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center;\r\n    overflow-y: scroll;\r\n    height: 88vh;\r\n  }\r\n  .top-list{\r\n    display: flex;\r\n    justify-content: center;\r\n    width: 60%;\r\n  }\r\n  .fl{\r\n    padding-top: 40px;\r\n    border-left: #d5d5d5 1px solid;\r\n    border-right: #d5d5d5 1px solid;\r\n    width: 25%;\r\n  }\r\n  .fr{\r\n    flex: 1;\r\n    padding: 3% 3%;\r\n  }\n</style>\n"],"sourceRoot":""}]}