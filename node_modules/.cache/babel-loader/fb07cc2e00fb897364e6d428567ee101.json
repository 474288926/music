=\"top-item\">\r\n          <div class=\"mt\"><a href=\"\">{{value.name}}</a></div>\r\n          <div class=\"mb\"><a  class=\"el-icon-video-play\" @click=\"select(value)\"></a><a href=\"\" class=\"el-icon-folder-add\"></a></div>\r\n        </div>\r\n      </div>\r\n      <li v-for=\"(item, index) in value.songs.slice(0, 10)\" :key=\"index\">\r\n        <span>{{index+1}}</span>\r\n        <a href=\"\" :style=\"active\">{{item.name}}</a>\r\n        <div class=\"list\" ><good-list-item @oneplay=\"oneplay(item, index)\"></good-list-item></div>\r\n      </li>\r\n      <li ><a href=\"\" style=\"text-align: right; margin-right: 10px; font-weight: 200px;\">查看全部 ></a></li>\r\n    </ul>\r\n  </div>\n</template>\n\n<script>\r\n  import GoodListItem from './GoodListItem.vue'\r\n  \r\n  export default{\r\n    name: 'GoodsList',\r\n    components:{\r\n      GoodListItem\r\n    },\r\n    data() {\r\n      return {\r\n          current: 0,\r\n          isshow: false,\r\n          active: ''\r\n      }\r\n    },\r\n    props: {\r\n      toplist: {\r\n        type: Array,\r\n        default(){\r\n          return []\r\n        }\r\n      }\r\n    },\r\n    methods:{\r\n      select(value) {\r\n        // console.log('123')\r\n        this.$emit('select', value)\r\n      },\r\n      oneplay(item, index) {\r\n        this.$emit('oneplay', item, index)\r\n      }\r\n    },\r\n    watch:{\r\n      toplist:{\r\n        deep: true,  // 深度监听\r\n        handler(newVal,oldVal) {\r\n          // console.log(newVal,oldVal)\r\n        }\r\n      }\r\n    }\r\n  }\n</script>\n\n<style scoped>\r\n  .goods-list{\r\n    margin-top: 20px;\r\n    background-color: #f5f5f5;\r\n    /* border: #e1e1e1 0.5px solid; */\r\n    display: flex;\r\n  }\r\n  .goods-list ul{\r\n    flex: 1;\r\n    display: flex-wrap;\r\n    text-align: center;\r\n    border: #d4d4d4 1px solid;\r\n  }\r\n  .goods-list ul li{\r\n    background-color: #e8e8e8;\r\n    display: flex;\r\n    height: 30px;\r\n    line-height: 30px;\r\n    padding-left: 10px;\r\n  }\r\n  .list{\r\n    display: none;\r\n  }\r\n  .goods-list ul li:hover .list{\r\n    display: block;\r\n  }\r\n  .goods-list ul li a{\r\n    color: #000000;\r\n    flex: 1;\r\n    text-align: left;\r\n    padding-left: 10px;\r\n    font-family:Verdana;\r\n    text-overflow:ellipsis;overflow: hidden;\r\n  }\r\n  .goods-list ul li span{\r\n    margin-left: 30px;\r\n    font-size: 16px;\r\n  }\r\n  .goods-list ul li:nth-child(-n+4) span{\r\n    color: #c10d0c;\r\n  }\r\n  \r\n  .goods-list ul li:nth-child(even){\r\n    background-color: #f5f5f5;\r\n  }\r\n  .goods-list ul:nth-child(2){\r\n    border-left: none;\r\n    border-right: none;\r\n  }\r\n  .top{\r\n    display: inline-flex;\r\n    padding: 20px 0;\r\n  }\r\n  .top-img{\r\n    box-shadow: 0 3px 5px #2B2B2B;\r\n  }\r\n  .top-img img{\r\n    width: 80px;\r\n  }\r\n  .top-item{\r\n    padding: 10px 10px;\r\n  }\r\n  .mb a{\r\n    font-size: 30px;\r\n    text-decoration: none;\r\n    color: #ccc;\r\n    margin-left: 10px;\r\n  }\r\n  .mb a:hover{\r\n    color: #555555;\r\n  }\r\n  .mt{\r\n    font-weight: bold;\r\n    font-size: 14px;\r\n  }\r\n  .mt a{\r\n    color: #000000;\r\n  }\r\n  .mb{\r\n   margin-top: 10px;\r\n  }\r\n  @media only screen and (max-width: 960px) {\r\n    .top{\r\n      flex-flow: row wrap;\r\n      align-items: center;\r\n      justify-content: center;\r\n    }\r\n    .top-item{\r\n      padding: 10px 0;\r\n    }\r\n    .goods-list ul li span{\r\n      margin-left: 0;\r\n    }\r\n  }\n</style>\n"],"sourceRoot":""}]}